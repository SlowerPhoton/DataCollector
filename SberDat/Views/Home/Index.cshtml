@{
    ViewBag.Title = "Data Collector";
}

<div class="row">
    <div class="col-md-4">
        <h2>Please log in</h2>
        <p>
            When logged in you can view and report the state of buildings under your control.
        </p>
            <form id="login" method="post" action="~/Home/Buildings">
